{
  "extends":[ "next/core-web-vitals","prettier"],
  "rules": {
    "prettier/prettier": [
      2,
      {
        "bracketSpacing": true,
        "printWidth": 120,
        "singleQuote": true,
        "trailingComma": "none",
        "tabWidth": 2,
        "useTabs": false,
        "endOfLine": "auto"
      }
    ],
    "curly": ["error", "multi-or-nest", "consistent"]
  }
}
